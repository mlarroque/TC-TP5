\begin_inset Formula $\begin{cases}
Ra1=1230\Omega & Rb=1650\Omega\\
Ra2=5230\Omega & C21=1nF\\
R41=1750\Omega & C22=1nF\\
R42=9400\Omega & C3=2nF\\
R1=2300\Omega
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
Celda 2:
\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
Ra1=10900\Omega & Rb=2525\Omega\\
Ra2=118000\Omega & C21=940pF\\
R41=3500\Omega & C22=3.9nF\\
R42=50000\Omega & C3=4.7nF\\
R1=570\Omega
\end{cases}$
\end_inset


